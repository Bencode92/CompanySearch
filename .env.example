# Configuration API Pappers
PAPPERS_API_KEY=your_api_key_here

# Configuration optionnelle pour les scripts avancés
# Départements par défaut (tous IDF si non spécifié)
# DEFAULT_DEPARTMENTS=75,77,78,91,92,93,94,95

# Codes NAF par défaut (78.20Z si non spécifié)
# DEFAULT_NAF_CODES=78.20Z,78.30Z

# Format d'export par défaut
# DEFAULT_FORMAT=csv

# Paramètres de performance
# BATCH_SIZE=100
# CONCURRENT_REQUESTS=3

# Filtres par défaut (0 = désactivé)
# MIN_CA=0
# MAX_CA=0
# MIN_EFFECTIF=0
# MAX_EFFECTIF=0

# Throttling API (millisecondes)
# API_DELAY_MS=200
# PAPPERS_DELAY_MS=120